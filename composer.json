{
    "name": "jdr/mailer-bundle",
    "type": "symfony-bundle",
    "description": "Bundle for jdr/mailer to provide integration with symfony",
    "keywords": [
        "jdr",
        "mailer-bundle"
    ],
    "homepage": "https://github.com/johanderuijter/mailer-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Johan de Ruijter",
            "email": "dev@johanderuijter.nl",
            "homepage": "http://johanderuijter.nl",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "jdr/mailer": "~0.0.1",
        "symfony/http-kernel": "^2.8|^3.0|^4.0",
        "symfony/dependency-injection": "^2.8|^3.0|^4.0",
        "symfony/config": "^2.8|^3.0|^4.0",
        "symfony/templating": "^2.8|^3.0|^4.0",
        "jdr/mailer-swift-mailer-bridge": "^0.1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.0",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "JDR\\MailerBundle\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JDR\\MailerBundle\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
